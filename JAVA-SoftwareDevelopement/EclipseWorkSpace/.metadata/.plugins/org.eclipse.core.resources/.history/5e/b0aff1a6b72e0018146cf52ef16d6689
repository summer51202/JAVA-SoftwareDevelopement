import java.util.Scanner;

public class HW2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner scanner = new Scanner(System.in);
		Double coef_a = 0.0;
		Double coef_b = 0.0;
		Double coef_c = 0.0;
		Double root_1 = 0.0;
		Double root_2 = 0.0;
		String numFormat = "";
		System.out.println("coef_a = " + coef_a);
		coef_a = scanner.nextDouble();
		System.out.println("coef_b = " + coef_b);
		coef_b = scanner.nextDouble();
		System.out.println("coef_c = " + coef_c);
		coef_c = scanner.nextDouble();
		System.out.println("Number Format is : " + numFormat);
		numFormat = scanner.nextLine();
		root_1 = (-coef_b + Math.sqrt(Math.pow(coef_b, 2) - 4*coef_a*coef_c))/(2*coef_a);
		root_2 = (-coef_b - Math.sqrt(Math.pow(coef_b, 2) - 4*coef_a*coef_c))/(2*coef_a);
		
		
		

	}

}
